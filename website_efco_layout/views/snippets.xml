<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <!-- website.s_banner -->
  <template id="change_s_banner" inherit_id="website.s_banner">
    <xpath expr="//div[@class='item oe_img_bg oe_custom_bg active']" position="attributes">
      <attribute name="style"></attribute>
    </xpath>
  </template>
  <!-- website.s_s_big_picture -->
  <template id="change_s_big_picture" inherit_id="website.s_big_picture">
    <xpath expr="//section[@class='s_big_picture bg-primary']" position="attributes">
      <attribute name="class">s_big_picture</attribute>
    </xpath>
  </template>
  
</odoo>