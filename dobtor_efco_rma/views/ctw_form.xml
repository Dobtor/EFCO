<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="mrp_repair_ctw">
            <field name="name">mrp.repair.form</field>
            <field name="model">mrp.repair</field>
            <field name="inherit_id" ref="mrp_repair.view_repair_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='guarantee_limit']" position="after">
                    <field name="CTM_ref"/>
                    <field name="CTW_PNLot"/>
                    <field name="CTW_AssemblyPO"/>
                    <field name="CTW_ChangeCPU"/>
                    <field name="CTW_Warranty"/>
                    <field name="CTW_Heat_sink"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>